{
    "manifest_version":2,
    "name":"ParentalControl",
    "version":"1.0.0",
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["jquery.js", "index.js"],
            "run_at": "document_end"
        }],
      "browser_action":{
      "default_popup":"popup.html"
    }
}
